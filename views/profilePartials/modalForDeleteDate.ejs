<!--Modal for creating Post------------------------------------------------------------------------------>
<div class="modal fade" id="deleteModalCenter" tabindex="-1" role="dialog" aria-labelledby="deleteModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header" style="background-color: #4e2cca; padding-left:10%; justify-content: center;">
            <h5 class="modal-title" style="font-family: 'Libre Baskerville', serif; color: cornsilk; font-style: bold;">Are you sure you want to delete it?</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>

      
               
        <div class="modal-body" style="position: relative;">
          
          
            <div class="text-center" style="align-content: center;justify-content: center;">
                <img src="/images/cancelDate.png" height="180px" width="250px" style="object-fit:cover" alt=" Are you sure?" class="rounded pt-10 " />
              </div>    
            

        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" id="d1" data-dismiss="modal">No,keep it</button>
         
          <a href="" type="submit" class="btn btn-primary" role="button" id="d2">Yes, I have made my mind.</a>
          
        </div>

    
      </div>
    </div>
</div>
